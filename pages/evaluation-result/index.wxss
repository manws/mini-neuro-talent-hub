page {
  height: 100%;
}

.container {
  align-items: flex-start;
  height: 100vh;
  background-color: #F6F6F6;
}

.header-layout {
  width: 100%;
  background-color: #01549C;
}

.toolbar-layout {
  width: 100%;
  justify-content: center;
  align-items: center;
}

.back-icon {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  left: 32rpx;
  transform: rotate(180deg);
}

.title-text {
  font-family: PingFangSC-Semibold;
  font-size: 36rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
  line-height: 50rpx;
  font-weight: 600;
}

.op-group-layout {
  width: 100%;
  justify-content: space-around;
  background-color: #FFFFFF;
  padding-bottom: env(safe-area-inset-bottom);
}

.op-text {
  width: 686rpx;
  height: 80rpx;
  border-radius: 56rpx;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #ffffff;
  letter-spacing: 0;
  text-align: center;
  line-height: 40rpx;
  font-weight: 600;
  background-color: #01549C;
  margin: 26rpx 0;
}

.success-layout {
  width: 100%;
  height: 0;
  flex-grow: 1;
  margin-top: 160rpx;
  align-items: center;
}

.success-text {
  font-family: PingFangSC-Semibold;
  font-size: 40rpx;
  color: #333333;
  letter-spacing: 0;
  line-height: 56rpx;
  font-weight: 600;
  margin-top: 32rpx;
}

.success-sub-text {
  font-family: PingFangSC-Regular;
  font-size: 40rpx;
  color: #333333;
  letter-spacing: 0;
  line-height: 40rpx;
  font-weight: 400;
}

.success-score-text {
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #01549C;
  letter-spacing: 0;
  line-height: 40rpx;
  font-weight: 500;
}

.doctor-icon {
  width: 222rpx;
  height: 222rpx;
}