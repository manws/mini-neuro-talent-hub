page {
  height: 100%;
}

.container {
  align-items: flex-start;
  height: 100vh;
  background-color: #F6F6F6;
}

.header-layout {
  width: 100%;
  background-color: #01549C;
}

.swiper-view {
  width: 686rpx;
  height: 1;
  flex-grow: 1;
  padding-top: 16rpx;
  margin-left: 32rpx;
  margin-top: 32rpx;
  margin-bottom: 80rpx;
  box-sizing: border-box;
}

.swiper-item-view {
  width: 686rpx;
  padding: 32rpx 24rpx;
  background-color: #FFFFFF;
  border-radius: 12rpx;
  box-sizing: border-box;
}

.scroll-view {
  height: 0;
  flex-grow: 1;
  margin-top: 32rpx;
}

.toolbar-layout {
  width: 100%;
  justify-content: center;
  align-items: center;
}

.back-icon {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  left: 32rpx;
  transform: rotate(180deg);
}

.title-text {
  font-family: PingFangSC-Semibold;
  font-size: 36rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
  line-height: 50rpx;
  font-weight: 600;
}

.op-group-layout {
  width: 100%;
  justify-content: space-around;
  padding-bottom: env(safe-area-inset-bottom);
}

.op-text {
  width: 323rpx;
  height: 80rpx;
  border-radius: 56rpx;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #ffffff;
  letter-spacing: 0;
  text-align: center;
  line-height: 40rpx;
  font-weight: 600;
  background-color: #01549C;
  margin: 26rpx 0;
}

.op-text-zero {
  width: 323rpx;
  height: 80rpx;
  border-radius: 56rpx;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: center;
  line-height: 40rpx;
  font-weight: 600;
  background-color: #EEEEEE;
  margin: 26rpx 0;
}

.op-text-pre {
  width: 323rpx;
  height: 80rpx;
  border-radius: 56rpx;
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #01549C;
  letter-spacing: 0;
  text-align: center;
  line-height: 40rpx;
  font-weight: 600;
  border: solid 1rpx #01549C;
  margin: 26rpx 0;
}

.input-layout {
  width: 634rpx;
  height: 88rpx;
  align-items: center;
  margin-top: 32rpx;
  border: solid 1rpx #EEEEEE;
}

.title {
  font-family: PingFangSC-Semibold;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 0;
  line-height: 46rpx;
  font-weight: 600;
}

.input {
  width: 100%;
  height: 100%;
  padding-left: 24rpx;
  color: #333333;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  letter-spacing: 0;
  line-height: 48rpx;
  font-weight: 400;
  caret-color: #01549C;
}

.tips {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #666666;
  letter-spacing: 0;
  line-height: 44rpx;
  font-weight: 400;
  margin-top: 32rpx;
}